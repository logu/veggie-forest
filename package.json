{
  "name": "veggie-forest",
  "version": "0.0.1",
  "dependencies": {
    "backbone": "^1.2.3",
    "backbone.marionette": "^2.4.3",
    "jquery": "~2.1.4",
    "normalize.css": "~3.0.3"
  },
  "browserify": {
    "transform": [
      "hbsfy"
    ]
  },
  "devDependencies": {
    "autoprefixer": "~6.0.3",
    "browser-sync": "~2.9.11",
    "browserify": "~12.0.0",
    "browserify-istanbul": "^0.2.1",
    "chai": "~3.4.0",
    "del": "~2.0.2",
    "envify": "~3.4.0",
    "glob": "~5.0.15",
    "gulp": "~3.9.0",
    "gulp-changed": "~1.3.0",
    "gulp-data": "~1.2.0",
    "gulp-eslint": "~1.0.0",
    "gulp-htmlmin": "~1.2.0",
    "gulp-if": "~2.0.0",
    "gulp-imagemin": "~2.3.0",
    "gulp-load-plugins": "~1.0.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-newer": "~1.0.0",
    "gulp-plumber": "~1.0.1",
    "gulp-postcss": "~6.0.1",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "~2.0.4",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-uglify": "~1.4.2",
    "gulp-util": "~3.0.7",
    "hbsfy": "^2.4.1",
    "imagemin-pngquant": "~4.2.0",
    "imagemin-svgo": "~4.1.2",
    "karma": "~0.13.14",
    "karma-browserify": "~4.4.0",
    "karma-chai": "~0.1.0",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "~0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "~0.2.1",
    "lodash": "~3.10.1",
    "minimist": "~1.2.0",
    "mocha": "~2.3.3",
    "phantomjs": "~1.9.0",
    "rimraf": "~2.4.3",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.5.0 ",
    "wrench": "~1.5.8"
  },
  "scripts": {
    "clean-deps": "rimraf node_modules"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "components": "_components",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}"
    }
  }
}
